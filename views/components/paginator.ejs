<form class="paginator" data-paginator>
  <!-- Add query if defined -->
  <% if (query) { %>
  <input type="hidden" name="query" value="<%- query %>" />
  <%}%>
  <!-- Previous -->
  <% if(pageCount == 1 ){ %> <% } else if ( page == 1) { %>
  <button
    class="paginator__control"
    name="page"
    value="<%- parseInt(page) - 1%>"
    disabled
  >
    <%- include('../assets/arrow', {direction:"right"}); %>
  </button>
  <% } else { %>
  <button
    class="paginator__control"
    name="page"
    value="<%- parseInt(page) - 1%>"
  >
    <%- include('../assets/arrow', {direction:"right"}); %>
  </button>
  <% } %>

  <!-- First -->
  <% if(pageCount == 1 ){ %>
  <button
    class="paginator__control paginator__control--active"
    name="page"
    value="1"
    disabled
  >
    1
  </button>
  <% } else if ( page == 1) { %>
  <button
    class="paginator__control paginator__control--active"
    name="page"
    value="1"
    disabled
  >
    1
  </button>
  <% } else { %>
  <button class="paginator__control" name="page" value="1">1</button>
  <% } %>

  <!-- Second -->
  <% if(pageCount <= 2 ){ %> <% } else if ( page >= 5) { %>
  <button class="paginator__control" disabled>...</button>
  <% } else if ( page == 2) { %>
  <button
    class="paginator__control paginator__control--active"
    name="page"
    value="2"
    disabled
  >
    2
  </button>
  <% } else { %>
  <button class="paginator__control" name="page" value="2">2</button>
  <% } %>

  <!-- Third -->
  <% if(pageCount <= 3 ){ %> <% } else if (page == pageCount || page ==
  (pageCount - 1) || page == (pageCount - 2) || page == (pageCount - 3)) { %>
  <button
    class="paginator__control"
    name="page"
    value="<%- parseInt(pageCount) - 4%>"
  >
    <%- parseInt(pageCount) - 4%>
  </button>
  <% } else if ( page >= 5) { %>
  <button
    class="paginator__control"
    name="page"
    value="<%- parseInt(page) - 1%>"
  >
    <%- parseInt(page) - 1%>
  </button>
  <% } else if ( page == 3) { %>
  <button
    class="paginator__control paginator__control--active"
    name="page"
    value="3"
    disabled
  >
    3
  </button>
  <% } else { %>
  <button class="paginator__control" name="page" value="3">3</button>
  <% } %>

  <!-- Fourth -->
  <% if(pageCount <= 4 ){ %> <% } else if (page == pageCount || page ==
  (pageCount - 1) || page == (pageCount - 2)) { %>
  <button
    class="paginator__control"
    name="page"
    value="<%- parseInt(pageCount) - 3%>"
  >
    <%- parseInt(pageCount) - 3%>
  </button>
  <% } else if ( page == (pageCount - 3)) { %>
  <button
    class="paginator__control paginator__control--active"
    name="page"
    value="<%- parseInt(pageCount) - 3%>"
    disabled
  >
    <%- parseInt(pageCount) - 3%>
  </button>
  <% } else if ( page >= 5) { %>
  <button
    class="paginator__control paginator__control--active"
    name="page"
    value="<%- parseInt(page)%>"
    disabled
  >
    <%- parseInt(page) %>
  </button>
  <% } else if ( page == 4) { %>
  <button
    class="paginator__control paginator__control--active"
    name="page"
    value="4"
    disabled
  >
    4
  </button>
  <% } else { %>
  <button class="paginator__control" name="page" value="4">4</button>
  <% } %>

  <!-- Fifth -->
  <% if(pageCount <= 5 ){ %> <% } else if (page == pageCount || page ==
  (pageCount - 1) || page == (pageCount - 3)) { %>
  <button
    class="paginator__control"
    name="page"
    value="<%- parseInt(pageCount) - 2%>"
  >
    <%- parseInt(pageCount) - 2%>
  </button>
  <% } else if ( page == (pageCount - 2)) { %>
  <button
    class="paginator__control paginator__control--active"
    name="page"
    value="<%- parseInt(pageCount) - 2%>"
    disabled
  >
    <%- parseInt(pageCount) - 2%>
  </button>
  <% } else if ( page >= 5) { %>
  <button
    class="paginator__control"
    name="page"
    value="<%- parseInt(page) + 1%>"
  >
    <%- parseInt(page) + 1%>
  </button>
  <% } else { %>
  <button class="paginator__control" name="page" value="5">5</button>
  <% } %>

  <!-- Sixth -->
  <% if(pageCount <= 6 ){ %> <% } else if ( page == pageCount) { %>
  <button
    class="paginator__control"
    name="page"
    value="<%- parseInt(page) - 1%>"
  >
    <%- parseInt(page) - 1%>
  </button>
  <% } else if (page == (pageCount - 2) || page == (pageCount - 3)) { %>
  <button
    class="paginator__control"
    name="page"
    value="<%- parseInt(pageCount) - 1%>"
  >
    <%- parseInt(pageCount) - 1%>
  </button>
  <% } else if ( page == (pageCount - 1)) { %>
  <button
    class="paginator__control paginator__control--active"
    name="page"
    value="<%- parseInt(pageCount) - 1%>"
  >
    <%- parseInt(pageCount) - 1%>
  </button>
  <% } else { %>
  <button class="paginator__control" disabled>...</button>
  <% } %>

  <!-- Last -->
  <% if(pageCount <= 7 ){ %> <% } else if ( page == pageCount) { %>
  <button
    class="paginator__control paginator__control--active"
    name="page"
    value="<%- parseInt(pageCount)%>"
  >
    <%- parseInt(pageCount)%>
  </button>
  <% } else { %>
  <button
    class="paginator__control"
    name="page"
    value="<%- parseInt(pageCount)%>"
  >
    <%- parseInt(pageCount)%>
  </button>
  <% } %>

  <!-- Next -->
  <% if(pageCount == 1 ){ %> <% } else if ( page == pageCount) { %>
  <button
    class="paginator__control"
    name="page"
    value="<%- parseInt(page) + 1%>"
    disabled
  >
    <%- include('../assets/arrow', {direction:"left"}); %>
  </button>

  <% } else { %>
  <button
    class="paginator__control"
    name="page"
    value="<%- parseInt(page) + 1%>"
  >
    <%- include('../assets/arrow', {direction:"left"}); %>
  </button>
  <% } %>

  <!-- Add pagesizes if defined -->
  <!-- <% if (pageSize) { %> -->
  <!-- <input type="hidden" name="pageSize" value="<%- pageSize %>"> -->
  <!-- <%}%> -->
</form>
